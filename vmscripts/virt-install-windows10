sudo virt-install \
	--name Windows10 \
	--os-type=Windows \
	--os-variant=win10 \
	--description "WIN10 VM Workstation" \
	--cdrom windows_10_pro_en-US_v1909_x64.iso \
	--disk path=vmWin10.qcow2,format=qcow2,sparse=no,size=60 \
	--vcpus=2 \
	--ram=3456 \
	--check-cpu \
	--arch=x86_64 \
	--virt-type kvm \
	--graphics spice \
	--network network:default
