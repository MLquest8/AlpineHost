sudo virt-install \
	--name Archer \
	--os-type=Linux \
	--os-variant=archlinux \
	--description "ARCH VM personal" \
	--cdrom ISO/archlinux-2021.09.01-x86_64.iso \
	--disk path=vmArcher.qcow2,format=qcow2,sparse=no,size=50 \
	--vcpus=2 \
	--ram=2048 \
	--check-cpu \
	--arch=x86_64 \
	--virt-type kvm \
	--graphics spice \
	--network network:default
